            </Menu>
        </Menu>
      <Menu mnemonicParsing="false" text="Compile and display output">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
        </items>
      </Menu>
    </MenuBar>
   <ComboBox fx:id="combo" onAction="#onkey" prefWidth="150.0" promptText="Suggessions" />
